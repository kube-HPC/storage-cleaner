{
  "name": "storage-cleaner",
  "version": "1.1.27",
  "description": "cleaner",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
    "test-travis": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec tests --colors --recursive --exit",
    "start": "node app",
    "build": "./dockerfile/build.sh storage-cleaner"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "keywords": [
    "hkube",
    "hkube-core"
  ],
  "author": "bahalool",
  "license": "MIT",
  "dependencies": {
    "@hkube/config": "^2.0.7",
    "@hkube/logger": "^1.0.15",
    "@hkube/storage-manager": "^1.0.46",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "mockery": "^2.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^5.1.1",
    "uuid": "^3.3.2"
  }
}
