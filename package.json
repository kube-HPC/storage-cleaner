{
  "name": "storage-cleaner",
  "version": "2.0.0",
  "description": "cleaner",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --timeout=20000 --colors --recursive --exit",
    "test-travis": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec tests --timeout=20000 --colors --recursive --exit",
    "start": "node app",
    "build": "./dockerfile/build.sh storage-cleaner"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "keywords": [
    "hkube",
    "hkube-core"
  ],
  "author": "Hkube",
  "license": "MIT",
  "dependencies": {
    "@hkube/config": "^2.0.7",
    "@hkube/logger": "^1.0.26",
    "@hkube/storage-manager": "^2.0.22",
    "moment": "^2.25.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^7.1.2",
    "mockery": "^2.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^9.0.2",
    "uuid": "^3.3.3"
  }
}
